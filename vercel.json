{
	"version":2,
	"routes": [
    {
      "src": "/api/rcon",
      "dest": "/api/rcon.js"
    },
		{
      "src": "/api/voucher",
      "dest": "/api/voucher.js"
    },
		{
      "src": "/api/przelew",
      "dest": "/api/przelew.js"
    },
		{
      "src": "/api/sms",
      "dest": "/api/sms.js"
    }
  ],
  "builds": [
		{
		 "src": "api/**/*.js",
		 "use": "@vercel/node"
	 	},
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
			"config": {
        "serverFiles": [
					"api/**",
					"package.json",
					".nuxt/dist/sitemap-routes.json"
				],
				"internalServer": true
      }
    }
  ]
}
